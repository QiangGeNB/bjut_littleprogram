.view-all{
  display: flex;
  width: 100%;
}
.background{
  position: absolute;
  top: 0px;
  left: 0;
  width: 100vw;
  height: 100%;
}
.content{
  position: relative;
  flex: 0 0 60vw;
  margin: 25vw auto;
  height: 75vw;
  padding: 20px;
  background: rgba(255,255,255,0.5);
  border-top-left-radius: 5%;
  border-bottom-right-radius: 5%;
}
.content2{
  opacity: 1;
}
.logo{
  position: absolute;
  opacity: 0.5;
  z-index: 1;
  width: 20vw;
  height: 20vw;
  right: 10%;
  top: 15%;
}

